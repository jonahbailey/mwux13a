!!! 5
%html
  %head
    %title Midwest UX 2013 | Grand Rapids, MI | Oct. 17-19
    %meta{:charset => "utf-8"}
    %meta{:content => "Midwest UX 2013", :name => "description"}
    <meta property="og:image" content="http://2013.midwestuxconference.com/images/logo.png" />
    %meta{:content => "on", "http-equiv" => "cleartype"}
    %meta{:content => "IE=edge,chrome=1", "http-equiv" => "X-UA-Compatible"}
    %meta{:content => "width=device-width", :name => "viewport"}
    %link{:href => "/stylesheets/style.css", :rel => "stylesheet", :type =>"text/css"}
    %script{:type => "text/javascript",
        :src  => "//use.typekit.net/diw1mze.js"}
    <script type="text/javascript">try{Typekit.load();}catch(e){}</script>
    %script{:src => "/javascripts/jquery-1.10.1.min.js"}
    %script{:src => "/javascripts/plugins/jquery.meanmenu.js"}
    %script{:src => "/javascripts/jquery.equal-height.js"}
    %script{:src => "/javascripts/underscore.min.js"}
    %script{:src => "/javascripts/hero.js"}


  %body.summer
    = render_partial 'header'
    
    .container.interior.blog
      .masthead.brainwaves.cf
        .title.cf
          %h1 Midwest UX
        .main.content.cf
    
          = yield
    
    = render_partial 'footer'
